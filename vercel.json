{
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/usersignup", "methods": ["POST"], "dest": "app.js" },
    { "src": "/userlogin", "methods": ["POST"], "dest": "app.js" },
    { "src": "/signup", "methods": ["GET"], "dest": "app.js" },
    { "src": "/login", "methods": ["GET"], "dest": "app.js" },
    { "src": "/", "methods": ["GET"], "dest": "app.js" }
  ]
}
}

